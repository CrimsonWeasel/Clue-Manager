<Application x:Class="ClueManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Shell.xaml"
             xmlns:Presenters="clr-namespace:ClueManager.Presenters"
             xmlns:Model="clr-namespace:ClueManager.Model">
    <Application.Resources>
        <ResourceDictionary>
            <ObjectDataProvider x:Key="originList"
                                MethodName="GetList"
                                ObjectType="{x:Type Model:OriginsViewModel}"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\ColorsAndBrushes.xaml"/>
                <ResourceDictionary Source="Resources\DefaultStyles.xaml"/>                
            </ResourceDictionary.MergedDictionaries>            
        </ResourceDictionary>
    </Application.Resources>
</Application>

